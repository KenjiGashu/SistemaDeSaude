<%= error_messages_for(@paciente) %>

<table summary="paciente">
	<tr>
		<th><%= f.label(:nome) %></th>
		<td><%= f.text_field(:nome) %></td>
	</tr>
	<tr>
		<th><%= f.label(:rg) %></th>
		<td><%= f.text_field(:rg) %></td>
	</tr>
	<tr>
		<th><%= f.label(:email) %></th>
		<td><%= f.text_field(:email) %></td>
	</tr>
	<tr>
		<th><%= f.label(:endereco) %></th>
		<td><%= f.text_area(:endereco, :size => '40x5') %></td>
	</tr>
	<tr>
		<th><%= f.label(:telefone) %></th>
		<td><%= f.text_field(:telefone) %></td>
	</tr>
	<tr>
		<th><%= f.label(:password) %></th>
		<td><%= f.password_field(:password) %></td>
	</tr>
	<tr>
		<th><%= f.label(:password_confirmation) %></th>
		<td><%= f.password_field(:password_confirmation) %></td>
	</tr>
</table>